<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
  <url><loc>{{config.absoluteBasePath}}</loc><lastmod>{{dateFormat pubDate 'iso8601'}}</lastmod><changefreq>daily</changefreq><priority>0.4</priority></url>
  {{#each index}}
    <url><loc>{{meta.AbsoluteUrl}}</loc><lastmod>{{dateFormat meta.Modified 'iso8601'}}</lastmod></url>
  {{/each}}
</urlset>
