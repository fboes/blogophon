<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
  <url><loc>{{config.absoluteBasePath}}</loc><lastmod>{{pubDate}}</lastmod><changefreq>daily</changefreq><priority>0.4</priority></url>
  <url><loc>{{config.absoluteBasePath}}{{config.htdocs.tag}}/</loc><lastmod>{{pubDate}}</lastmod><changefreq>daily</changefreq><priority>0.3</priority></url>
  {{#index}}
    <url><loc>{{meta.AbsoluteUrl}}</loc><lastmod>{{meta.Created.iso}}</lastmod></url>
    {{#config.specialFeatures.acceleratedmobilepages}}
      <url><loc>{{meta.AbsoluteUrlAmp}}</loc><lastmod>{{meta.Created.iso}}</lastmod></url>
    {{/config.specialFeatures.acceleratedmobilepages}}
  {{/index}}
  {{#tagPages}}
    <url><loc>{{config.baseUrl}}{{url}}</loc><lastmod>{{pubDate}}</lastmod><changefreq>daily</changefreq><priority>0.2</priority></url>
  {{/tagPages}}
</urlset>
